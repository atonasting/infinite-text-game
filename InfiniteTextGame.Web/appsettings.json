{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "TimestampFormat": "[MM-dd HH:mm:ss] "
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SQLiteDb": "Data Source=Db/itg.db"
  },

  "WebProxy": "", //optional, if your server blocked by openai: http://ip:port or socks5://ip:port

  "Type": "OpenAI", //OpenAI or Azure
  //for OpenAI
  "OpenAIApiKey": "", //api key
  "DefaultModel": "", //custom model name like 'gpt-3.5-turbo-16k-0613' or 'gpt-4-0613' etc.
  //for Azure
  "AzureApiKey": "", //api key
  "ResourceName": "", //resource name for endpoint: https://{ResourceName}.openai.azure.com/
  "DeploymentId": "" //deployment id in azure openAi studio
}
